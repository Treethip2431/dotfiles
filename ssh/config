# vim: set ft=sshconfig :

Include ~/.ssh/work-config

Host *
  ControlPath ~/.ssh/master-%r@%h:%p
  ControlMaster auto
  ControlPersist 3h

Host ark
  HostName ark.lan
  Port 10022
  User admin

Host esther
  HostName esther.ipn.willnorris.net
  User elisabeth

Host levi
  HostName levi.ipn.willnorris.net
  User willnorris

Host local
  HostName levi.ipn.willnorris.net
  User willnorris
  RemoteForward 9200 localhost:8080

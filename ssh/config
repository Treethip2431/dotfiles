# vim: set ft=sshconfig :

Include ~/.ssh/google-config

Host *
  ControlPath ~/.ssh/master-%r@%h:%p
  ControlMaster auto

Host home
  HostName home.willnorris.net
  Port 10022
  User elisabeth

Host ark
  HostName ark.lan
  Port 10022
  User admin

Host esther
  HostName esther.lan
  User elisabeth

Host levi
  HostName levi.willnorris.net
  User willnorris

# vim: set ft=sshconfig :

ConnectionAttempts 3
SendEnv COLORFGBG

Host *
  IdentityFile ~/.ssh/id_rsa

  ControlPath ~/.ssh/master-%r@%h:%p
  ControlMaster auto


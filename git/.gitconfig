[core]
  excludesfile = ~/.gitignore
  quotepath = false

[diff "vim"]
  command = vimdiff

[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto

[init]
  templatedir = ~/.git_template

[alias]
  ctags = !.git/hooks/ctags

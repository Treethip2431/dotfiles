[user]
  email = will@willnorris.com
  name = Will Norris

[core]
  excludesfile = ~/.gitignore
  quotepath = false

[diff "vim"]
  command = vimdiff

[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto

[init]
  #templatedir = ~/.git_template

[alias]
  ctags = !.git/hooks/ctags
  #sort = !git5 diff --name-only | grep -e '\\.java$' | xargs READONLY/google3/tools/java/sort_java_imports.py
  #unused = !git5 diff --name-only | grep -e '\\.java$' | xargs READONLY/google3/tools/java/remove_unused_imports.py --fix
  #buildify = !git5 diff --name-only | grep -e 'BUILD$' | xargs /google/data/ro/projects/orkut/util/buildifier.par --mode=fix
  #meld = !/home/willnorris/git-meld
[rerere]
  #enabled = true
